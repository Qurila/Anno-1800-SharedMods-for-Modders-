<ModOps>
  <!-- 1500301992 reserved but not yet used -->

  <ModOp Type="add" Path="/TextExport/Texts">
    <!-- InputAmountUpgrade title -->
    <Text>
      <GUID>1500301995</GUID>
      <Text>Biens d'entrée</Text>
    </Text>
    <!-- positive InputAmountUpgrade -->
    <Text>
      <GUID>1500301996</GUID>
      <Text>[AssetData([ItemAssetData([RefGuid]) InputAmountUpgradeProduct(#index0#)]) Text] &lt;img height='24' width='24' src="[ItemAssetData([ItemAssetData([RefGuid]) InputAmountUpgradeProduct(#index0#)]) Icon]"/&gt; +[ItemAssetData([RefGuid]) InputAmountUpgradeAmount(#index0#)]</Text>
    </Text>
    <!-- negative InputAmountUpgrade -->
    <Text>
      <GUID>1500301997</GUID>
      <Text>[AssetData([ItemAssetData([RefGuid]) InputAmountUpgradeProduct(#index0#)]) Text] &lt;img height='24' width='24' src="[ItemAssetData([ItemAssetData([RefGuid]) InputAmountUpgradeProduct(#index0#)]) Icon]"/&gt; [ItemAssetData([RefGuid]) InputAmountUpgradeAmount(#index0#)]</Text>
    </Text>

    <!-- ReplaceInput remains GUID 20081 -->
    <!-- ReplaceInput not abstract -->
    <Text>
      <GUID>1500301993</GUID>
      <Text>&lt;b&gt;[AssetData([ItemAssetData({ItemGuid}) ReplaceInputNewInput(#index0#)]) Text]&lt;/b&gt; &lt;img height='24' width='24' src="[ItemAssetData([ItemAssetData({ItemGuid}) ReplaceInputNewInput(#index0#)]) Icon]"/&gt;&lt;font overrideTextColor="true" color='#eadd99'&gt;&#xD;&#xA;remplace&lt;/font&gt;&#xD;&#xA;&lt;b&gt;[AssetData([ItemAssetData({ItemGuid}) ReplaceInputOldInput(#index0#)]) Text]&lt;/b&gt; &lt;img height='24' width='24' src="[ItemAssetData([ItemAssetData({ItemGuid}) ReplaceInputOldInput(#index0#)]) Icon]"/&gt;</Text>
    </Text>
    <!-- replace input abstract -->
    <Text>
      <GUID>1500301994</GUID>
      <Text>&lt;b&gt;[AssetData([ItemAssetData({ItemGuid}) ReplaceInputNewInput(#index0#)]) Text]&lt;/b&gt; &lt;img height='24' width='24' src="[ItemAssetData([ItemAssetData({ItemGuid}) ReplaceInputNewInput(#index0#)]) Icon]"/&gt; est maintenant traité.</Text>
    </Text>

  </ModOp>



</ModOps>