    <!-- how this mod works: -->
    
    <!-- Unfortunately the game only allows us to execute triggers for: "Only Humans" or "Every Huamnd and every AI". There is no proper way to check for who exactly -->
    <!--  a trigger is executed and no way to execute triggers only for AI (triggers are never executed for 3rd parties, eg. pirates. You can do stuff with the pirates, but then it is done from the view of the executing player)  -->
    <!-- For this I only know a workaround to check if the processing (owner) Player has a AI-lighthouse GUID build. This is done by adding subtriggers. -->
    <!-- We will use triggers to check if the processing/owner does own any of the lighthouses (so of course this can only find specific AIs the lighthouse GUID is known, eg. mod-AIs must bei added to this first) -->
    <!-- Lighthouse GUIDs -->
    <!-- <GUID>100549</GUID> Jorgensens Leuchtturm , Second_ai_01_Jorgensen -->
    <!-- <GUID>100700</GUID> Qings Leuchtturm , Second_ai_02_Qing -->
    <!-- <GUID>100701</GUID> Wibblesocks' Leuchtturm , Second_ai_03_Wibblesock -->
    <!-- <GUID>100702</GUID> Smiths Leuchtturm , Second_ai_04_Smith -->
    <!-- <GUID>100703</GUID> O'Maras Leuchtturm , Second_ai_05_OMara -->
    <!-- <GUID>100704</GUID> Gasparovs Leuchtturm , Second_ai_06_Gasparov -->
    <!-- <GUID>100518</GUID> von Malchings Leuchtturm , Second_ai_07_von_Malching -->
    <!-- <GUID>100550</GUID> Graves Leuchtturm , Second_ai_08_Gravez -->
    <!-- <GUID>100705</GUID> Silvas Leuchtturm , Second_ai_09_Silva -->
    <!-- <GUID>100706</GUID> Hunts Leuchtturm , Second_ai_10_Hunt -->
    <!-- <GUID>110669</GUID> Merciers Leuchtturm , Second_ai_11_Mercier -->
    <!-- In addition we will add a "Is Human" condition, which is automatically true for all humans using the UsedBySecondParties=0 Trigger execution. -->
    <!-- So in the end everyone that has not IsHuman and owns a lighthouse must be an AI -->
    
    <Include File="/data/config/export/main/asset/LighthouseFix.include.xml" />
    
    <!-- FeatureUnlock to check if a specific Participant is AI -->
    <ModOp Type="AddNextSibling" GUID="137108">
        <Asset>
          <Template>FeatureUnlock</Template>
          <Values>
            <Standard>
              <GUID>1500001600</GUID>
              <Name>Is Human</Name>
              <IconFilename>data/ui/2kimages/main/profiles/playerlogos/small/player_logo_03.png</IconFilename>
            </Standard>
            <Locked />
            <Trigger>
              <TriggerCondition>
                <IsBaseAutoCreateAsset>1</IsBaseAutoCreateAsset>
                <Values>
                  <Condition />
                  <ConditionAlwaysTrue />
                </Values>
              </TriggerCondition>
            </Trigger>
            <TriggerSetup>
              <AutoRegisterTrigger>1</AutoRegisterTrigger>
              <UsedBySecondParties>0</UsedBySecondParties>
              <AutoSelfUnlock>1</AutoSelfUnlock>
            </TriggerSetup>
          </Values>
        </Asset>
        
        <!-- use a short timer for the AI check to be sure the "Is Human" Feature is already unlocked for all humans (cause everyone who does not have it unlocked is an AI) -->
        <Asset>
          <Template>FeatureUnlock</Template>
          <Values>
            <Standard>
              <GUID>1500001601</GUID>
              <Name>Is AI</Name>
