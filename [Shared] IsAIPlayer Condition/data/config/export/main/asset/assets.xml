    <!-- uses Serp GUIDS from 1500001600 to 1500001619 including (currently in use up to 1500001612)  -->
    
    
    <!-- USAGE: use the GUID from list below for the AI you want to check and use this condition in your code: -->
    <!-- <TriggerCondition> -->
      <!-- <Template>ConditionUnlocked</Template> -->
      <!-- <Values> -->
        <!-- <Condition /> -->
        <!-- <ConditionUnlocked> -->
          <!-- <UnlockNeeded>143532</UnlockNeeded> -->
        <!-- </ConditionUnlocked> -->
        <!-- <ConditionPropsNegatable /> -->
      <!-- </Values> -->
    <!-- </TriggerCondition> -->
    
    <!-- The AI Unlocks are true for AIs after 1 second of gametime. So dont use a construct with NegateCondition to check "Is not AI" directly at gamestart, because this will give false results. -->
    <!-- But you can use all of it without NegateCondition anytime, it will just take 1 second until the trigger executed eg. for the AI -->
    <!-- The GUIDs to use from this mod are: -->
    <!-- 1500001600 - Is Human -->
    <!-- 1500001601 - Is AI -->
    <!-- 1500001602 - Second_ai_01_Jorgensen -->
    <!-- 1500001603 - Second_ai_02_Qing -->
    <!-- 1500001604 - Second_ai_03_Wibblesock -->
    <!-- 1500001605 - Second_ai_04_Smith -->
    <!-- 1500001606 - Second_ai_05_OMara -->
    <!-- 1500001607 - Second_ai_06_Gasparov -->
    <!-- 1500001608 - Second_ai_07_von_Malching -->
    <!-- 1500001609 - Second_ai_08_Gravez -->
    <!-- 1500001610 - Second_ai_09_Silva -->
    <!-- 1500001611 - Second_ai_10_Hunt -->
    <!-- 1500001612 - Second_ai_11_Mercier -->
    
    
    
